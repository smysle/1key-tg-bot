# Telegram Bot Token (从 @BotFather 获取)
TG_BOT_TOKEN=your_telegram_bot_token

# 1Key API Key (用于绕过 hCaptcha)
ONEKEY_API_KEY=your_1key_api_key

# Redis 配置 (可选，用于持久化统计数据)
# 不配置则使用内存存储，重启后数据丢失
REDIS_URL=redis://localhost:6379/0

# 管理员用户ID (多个用逗号分隔)
# 默认: 6997010290
ADMIN_USER_IDS=6997010290

# CSRF Token 刷新间隔(秒)
CSRF_REFRESH_INTERVAL=300

# 每日用户限制 (0=不限制)
USER_DAILY_LIMIT=0
